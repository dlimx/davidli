{
  "name": "davidli",
  "description": "A starter kit for TypeScript-based Gatsby projects with sensible defaults.",
  "version": "1.0.0",
  "private": true,
  "author": "David Li <me@davidli.io>",
  "keywords": [
    "blog"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build --prefix-paths",
    "clean": "gatsby clean && rimraf public",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "dev": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md}\"",
    "lint": "eslint '**/*.{js,jsx,ts,tsx}' --fix",
    "start": "npm run dev",
    "test": "npm run typecheck && npm run lint",
    "typecheck": "tsc",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "@blocks/kit": "^0.0.28",
    "@emotion/react": "^11.1.4",
    "@emotion/styled": "^11.0.0",
    "@fontsource/open-sans": "^4.1.0",
    "@fontsource/rubik": "^4.1.0",
    "@mdx-js/mdx": "^1.1.0",
    "@mdx-js/react": "^1.0.27",
    "@raae/gatsby-remark-oembed": "^0.1.1",
    "adm-zip": "^0.4.13",
    "babel-plugin-emotion": "^10.0.14",
    "gatsby": "^3.15.0",
    "gatsby-paginate": "^1.1.0",
    "gatsby-plugin-emotion": "^6.15.0",
    "gatsby-plugin-feed": "^3.15.0",
    "gatsby-plugin-google-gtag": "^3.15.0",
    "gatsby-plugin-image": "^1.15.0",
    "gatsby-plugin-mailchimp": "^5.1.2",
    "gatsby-plugin-manifest": "^3.15.0",
    "gatsby-plugin-mdx": "^1.0.32",
    "gatsby-plugin-offline": "^4.15.0",
    "gatsby-plugin-react-helmet": "^4.15.0",
    "gatsby-plugin-sharp": "^3.15.0",
    "gatsby-plugin-theme-ui": "^0.12.1",
    "gatsby-plugin-typescript": "^3.15.0",
    "gatsby-remark-autolink-headers": "^3.2.0",
    "gatsby-remark-check-links": "^1.1.5",
    "gatsby-remark-copy-linked-files": "^3.2.0",
    "gatsby-remark-embed-video": "^3.2.0",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "^4.2.0",
    "gatsby-remark-numbered-footnotes": "^1.0.1",
    "gatsby-remark-prismjs": "^4.2.0",
    "gatsby-remark-smartypants": "^3.2.0",
    "gatsby-remark-typescript": "0.2.2",
    "gatsby-source-filesystem": "^3.15.0",
    "gatsby-transformer-remark": "^3.2.0",
    "gatsby-transformer-sharp": "^3.15.0",
    "gatsby-transformer-yaml": "^3.15.0",
    "lodash": "^4.17.15",
    "polish": "^0.2.3",
    "polished": "^4.0.5",
    "prism-react-renderer": "^0.1.7",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-live": "^2.2.0",
    "react-medium-image-zoom": "^4.0.2",
    "react-outside-click-handler": "^1.2.4",
    "remark-slug": "^5.1.2",
    "request": "^2.88.0",
    "slugify": "^1.3.6",
    "theme-ui": "^0.11.3",
    "typescript": "^3.5.2"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.8",
    "@types/mdx-js__react": "^1.5.3",
    "@types/node": "^12.0.10",
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.8.4",
    "@types/react-helmet": "^5.0.8",
    "@types/react-outside-click-handler": "^1.3.0",
    "@types/theme-ui": "^0.3.7",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "eslint": "^7.16.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "gh-pages": "^2.0.1",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3"
  },
  "resolutions": {
    "postcss-loader": "^7.0.2"
  },
  "lint-staged": {
    "*.js?(x)": "eslint --cache --fix",
    "*.ts?(x)": "eslint --cache --fix"
  }
}
